generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Spell {
  id                     String   @id @default(auto()) @map("_id") @db.ObjectId
  /// Could not determine type: the field only had null or empty values in the sample set.
  castingTime            Json?
  /// Could not determine type: the field only had null or empty values in the sample set.
  category               Json?
  /// Could not determine type: the field only had null or empty values in the sample set.
  characterId            Json?
  components             String[]
  description            String
  duration               String
  /// Could not determine type: the field only had null or empty values in the sample set.
  higherLevelDescription Json?
  level                  BigInt
  material               String?
  /// Could not determine type: the field only had null or empty values in the sample set.
  multipliciy            Json?
  name                   String
  range                  String
  ritual                 Boolean
  shortDescription       String?
}
